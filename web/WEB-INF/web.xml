<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControladorUsuario</servlet-name>
        <servlet-class>Controlador.ControladorUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorLogin</servlet-name>
        <servlet-class>Controlador.ControladorLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorCuenta</servlet-name>
        <servlet-class>Controlador.ControladorCuenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorFavorito</servlet-name>
        <servlet-class>Controlador.ControladorFavorito</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorDeposito</servlet-name>
        <servlet-class>Controlador.ControladorDeposito</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorRetiro</servlet-name>
        <servlet-class>Controlador.ControladorRetiro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorUsuario</servlet-name>
        <url-pattern>/ControladorUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorLogin</servlet-name>
        <url-pattern>/ControladorLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorCuenta</servlet-name>
        <url-pattern>/ControladorCuenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorFavorito</servlet-name>
        <url-pattern>/ControladorFavorito</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorDeposito</servlet-name>
        <url-pattern>/ControladorDeposito</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorRetiro</servlet-name>
        <url-pattern>/ControladorRetiro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
